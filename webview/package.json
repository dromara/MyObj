{
  "name": "myobj-webview",
  "private": true,
  "version": "1.0.0",
  "description": "MyObj网盘系统",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .eslintignore",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx,vue,json,css,scss,md}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"src/**/*.{js,ts,jsx,tsx,vue,json,css,scss,md}\" --ignore-path .prettierignore",
    "code-check": "npm run type-check && npm run lint:check && npm run format:check",
    "analyze": "npm run build:prod && node scripts/build-analysis.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@vueuse/core": "^14.1.0",
    "axios": "^1.13.2",
    "driver.js": "^1.4.0",
    "element-plus": "^2.11.7",
    "jsencrypt": "^3.5.4",
    "pinia": "^3.0.4",
    "plyr": "^3.8.3",
    "spark-md5": "^3.0.2",
    "vue": "^3.5.24",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/spark-md5": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/typescript-plugin": "^3.1.3",
    "autoprefixer": "^10.4.23",
    "code-inspector-plugin": "^1.3.3",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.2",
    "vite-plugin-compression": "^0.5.1"
  }
}
